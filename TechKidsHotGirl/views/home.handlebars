<div class="container content">
    <div class="grid" id="girl-list-container">
        <div class="gutter-sizer"></div>
    </div>
</div>

<script id="girl-list-template" type="text/x-handlebars-template">
    \{{#each images}}
    <div class="grid-item">
        <img src="\{{imageUrl}}" alt="" class="img img-fluid">
        <h3>\{{title}}</h3>
    </div>
    \{{/each}}
</script>

<script src="/static/js/home.js"></script>

