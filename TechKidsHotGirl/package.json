    "node-uuid": "^1.4.8"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.0.37",
    "@types/express-handlebars": "0.0.29",
    "@types/mongoose": "^4.7.24",
    "@types/node": "^8.0.33",
    "@types/node-uuid": "0.0.28",
    "jasmine": "^2.8.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "ts-node": "^3.3.0",
